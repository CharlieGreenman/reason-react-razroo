{
  "name": "reason-react-razroo",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "test": "jest --coverage && npm run coverage",
    "test-watch": "jest --coverage --watch",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov",
    "watch:jest": "jest --coverage --watchAll",
    "webpack": "webpack -w",
    "webpack:production": "NODE_ENV=production webpack"
  },
  "keywords": [
    "BuckleScript"
  ],
  "jest": {
    "testMatch": [
      "**/?(*_)(spec|test).bs.js?(x)"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.bs.js"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reason-react": ">=0.3.4"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.2",
    "bs-platform": "^2.2.3",
    "coveralls": "^3.0.0",
    "jest": "^22.4.3",
    "nyc": "^11.6.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10"
  }
}
